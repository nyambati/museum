{
	"name": "adonis-fullstack-app",
	"version": "4.1.0",
	"adonis-version": "4.1.0",
	"description": "The fullstack application boilerplate for Adonisjs",
	"main": "index.js",
	"scripts": {
		"start": "node server.js",
		"test": "node ace test",
		"build:prod": "parcel build src/index.html -d public --no-source-maps",
		"build:staging": "parcel build src/index.html -d public",
		"dev:web": "parcel src/index.html --open --port 8000",
		"dev:server": "adonis serve --dev",
		"dev": "npm-run-all --parallel dev:*"
	},
	"keywords": [
		"adonisjs",
		"adonis-app"
	],
	"author": "",
	"license": "UNLICENSED",
	"private": true,
	"dependencies": {
		"@adonisjs/ace": "^5.0.8",
		"@adonisjs/auth": "^3.0.7",
		"@adonisjs/bodyparser": "^2.0.5",
		"@adonisjs/cors": "^1.0.7",
		"@adonisjs/fold": "^4.0.9",
		"@adonisjs/framework": "^5.0.9",
		"@adonisjs/ignitor": "^2.0.8",
		"@adonisjs/lucid": "^6.1.3",
		"@adonisjs/session": "^1.0.27",
		"@adonisjs/shield": "^1.0.8",
		"axios": "^0.19.0",
		"babel-polyfill": "^6.26.0",
		"mysql": "^2.17.1",
		"pg": "^7.12.1",
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-js-pagination": "^3.0.2",
		"react-redux": "^7.1.1",
		"react-router-dom": "^5.1.2",
		"redux": "^4.0.4",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"shelljs": "^0.8.3",
		"sqlite3": "^4.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0-0",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"cssnano": "^4.1.10",
		"npm-run-all": "^4.1.5"
	},
	"autoload": {
		"App": "./app"
	}
}
